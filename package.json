{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev:klj": "cross-env TENANT_ID=KLJPAPINP001 node ./scripts/fetch-tenant.js && expo start -c",
    "dev:laxmeepay": "cross-env TENANT_ID=LAXMEPINP001 node ./scripts/fetch-tenant.js && expo start -c",
    "dev:divanshu": "cross-env TENANT_ID=90f55330-b4ec-4220-a59a-d4c1a459c0a5 node ./scripts/fetch-tenant.js && expo start -c",
    "dev:pinepe": "cross-env TENANT_ID=95c1fbeb-388c-44f4-88a9-dd4148f1df3c node ./scripts/fetch-tenant.js && expo start -c",
    "dev:nkpay": "cross-env TENANT_ID=daccd57e-c327-43cf-9f90-db7d72325454 node ./scripts/fetch-tenant.js && expo start -c",
    "build:klj": "cross-env TENANT_ID=KLJPAPINP001 node ./scripts/fetch-tenant.js && npx eas build --profile klj --platform android",
    "build:laxmeepay": "cross-env TENANT_ID=LAXMEPINP001 node ./scripts/fetch-tenant.js && npx eas build --profile laxmeepay --platform android",
    "build:divanshu": "cross-env TENANT_ID=90f55330-b4ec-4220-a59a-d4c1a459c0a5 node ./scripts/fetch-tenant.js && npx eas build --profile divanshu --platform android",
    "build:pinepe": "cross-env TENANT_ID=95c1fbeb-388c-44f4-88a9-dd4148f1df3c node ./scripts/fetch-tenant.js && npx eas build --profile pinepe --platform android",
    "build:nkpay": "cross-env TENANT_ID=daccd57e-c327-43cf-9f90-db7d72325454 node ./scripts/fetch-tenant.js && npx eas build --profile nkpay --platform android"
  },
